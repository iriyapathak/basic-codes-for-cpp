#include<iostream>
using namespace std;
int swap(int arr[] , int n){
    int a=0;
    int b=n-1;
    while (a<=b){
    swap(arr[a],arr[b]);
         a++;
        b--;}
     cout<<"reversed array: ";
    for(int i=0;i<n;i++){
        cout<<arr[i]; 
    }}
int main(){
    int n = 5;
int arr[]={1,2,3,4,5};
cout<<"array elemnets: ";
 for(int i=0;i<n;i++){
cout<<arr[i];
}cout<<endl;
int result=swap(arr,n);
}
